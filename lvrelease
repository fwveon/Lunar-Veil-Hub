-- CONFIGURATION
local correctKey = "Veilxd"
local keyLink = "https://link-target.net/1334888/keysystem"

-- Kavo UI Library Setup
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local KeyWindow = Library.CreateLib("Lunar Veil Key System", "Ocean")
local KeyTab = KeyWindow:NewTab("Key System")
local KeySection = KeyTab:NewSection("Access Required (Key Changes Everyday)")

local userInputKey = ""

-- Make Key GUI draggable
task.wait(1)
for _, gui in pairs(game:GetService("CoreGui"):GetChildren()) do
    if gui:FindFirstChild("Main") then
        local main = gui:FindFirstChild("Main")
        if main:IsA("Frame") then
            main.Active = true
            main.Draggable = true
        end
    end
end

-- Button to get key
KeySection:NewButton("Get Key", "Copies key website link", function()
    setclipboard(keyLink)
    game.StarterGui:SetCore("SendNotification", {
        Title = "Key Copied!",
        Text = "Paste the website link to your browser to get the key.",
        Duration = 6
    })
end)

-- Textbox to enter key
KeySection:NewTextBox("Enter Key", "Paste key here", function(txt)
    userInputKey = txt
end)

-- Validate key
KeySection:NewButton("Enter Key", "Submit your key", function()
    if userInputKey == correctKey then
        for _, gui in pairs(game:GetService("CoreGui"):GetChildren()) do
            if gui.Name == "KavoUI" then
                gui:Destroy()
            end
        end

        -- MAIN UI START
        local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("Lunar Veil's Hub", "Ocean")
        local TweenService = game:GetService("TweenService")

        task.wait(1)
        for _, gui in pairs(game:GetService("CoreGui"):GetChildren()) do
            if gui:FindFirstChild("Main") then
                local main = gui:FindFirstChild("Main")
                main.Position = UDim2.new(0.5, -250, -1, 0)
                local tween = TweenService:Create(main, TweenInfo.new(1, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                    Position = UDim2.new(0.5, -250, 0.5, -200)
                })
                tween:Play()
                main.Active = true
                main.Draggable = true

                local minimize = Instance.new("TextButton")
                minimize.Size = UDim2.new(0, 25, 0, 25)
                minimize.Position = UDim2.new(1, -60, 0, 5)
                minimize.Text = "_"
                minimize.BackgroundColor3 = Color3.fromRGB(0, 85, 127)
                minimize.TextColor3 = Color3.new(1, 1, 1)
                minimize.Font = Enum.Font.GothamBold
                minimize.TextSize = 18
                minimize.Parent = main

                local label = Instance.new("TextLabel", main)
                label.Size = UDim2.new(0, 200, 0, 25)
                label.Position = UDim2.new(0, 10, 0, 5)
                label.BackgroundTransparency = 1
                label.TextColor3 = Color3.new(1, 1, 1)
                label.Font = Enum.Font.GothamBold
                label.TextSize = 14
                label.Text = "Lunar Veil Hub"
                label.Visible = false

                local isMinimized = false
                local originalSize = main.Size
                local originalChildren = {}

                for _, child in pairs(main:GetChildren()) do
                    if child:IsA("GuiObject") and child ~= minimize and child ~= label then
                        table.insert(originalChildren, child)
                    end
                end

                minimize.MouseButton1Click:Connect(function()
                    isMinimized = not isMinimized
                    if isMinimized then
                        for _, c in pairs(originalChildren) do c.Visible = false end
                        label.Visible = true
                        label.Text = "Lunar Veil Hub (Minimized)"
                        main:TweenSize(UDim2.new(0, 250, 0, 50), "Out", "Sine", 0.4, true)
                    else
                        for _, c in pairs(originalChildren) do c.Visible = true end
                        label.Visible = false
                        main:TweenSize(originalSize, "Out", "Sine", 0.4, true)
                    end
                end)
            end
        end

